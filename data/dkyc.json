      "title": "FaceMatch API",
      "description": "Determines if a customer image matches the document image.",
      "endpoint": "POST /v1/face_match",
      "source": "Manappuram API Spec",
      "curl": "curl -X POST {{URL}}/v1/face_match \\\n-H 'Authorization: Bearer <auth token>' \\\n-F image=@1stimage.jpg \\\n-F image2=@2ndimage.jpg \\\n-F app_id=dkyc \\\n-F referenceId=123456 \\\n-F match_type=1 \\\n-F app_type=A",
      "inputs": [
        {"Param": "image", "M/O": "M", "Description": "Original face image"},
        {"Param": "image2", "M/O": "O", "Description": "Comparison face image"},
        {"Param": "match_type", "M/O": "M", "Description": "0: Customer-Doc, 1: Agent-Customer, 2: Agent-Agent"}
      ],
      "outputs": {
        "result": {
          "conf": "90",
          "match": "yes",
          "match-score": "92",
          "to-be-reviewed": "no"
