[
    {
      "title": "Liveness API",
      "description": "Determines if a customer image captured is live or a spoof.",
      "endpoint": "POST /v1/check_liveness",
      "source": "Manappuram API Spec",
      "curl": "curl -X POST {{URL}}/v1/check_liveness \\\n-H 'Authorization: Bearer <auth token>' \\\n-F image=@1stimage.jpg \\\n-F image2=@2ndimage.jpg \\\n-F app_id=app_id \\\n-F referenceId=123456 \\\n-F whiteBackground=1 \\\n-F eyesCheck=1 \\\n-F capture_type=0 \\\n-F app_type=A",
      "inputs": [
        {"Param": "image", "M/O": "M", "Description": "Original live image"},
        {"Param": "app_id", "M/O": "M", "Description": "Application ID"},
        {"Param": "referenceId", "M/O": "M", "Description": "User ID / ORN"},
        {"Param": "capture_type", "M/O": "M", "Description": "0 - Customer, 1 - Agent"},
        {"Param": "eyesCheck", "M/O": "O", "Description": "0 - Enable, 1 - Disable"},
        {"Param": "whiteBackground", "M/O": "O", "Description": "0 - Enable, 1 - Disable"}
      ],
      "outputs": {
        "checksum": "0adf39c99f...",
        "result": {
          "live": "yes",
          "liveness-score": "92",
          "to-be-reviewed": "no"
        },
        "status": "success",
        "statusCode": "200"
      }
    },
    {
      "title": "Aadhaar Read Document API",
      "description": "Verifies Aadhaar card, detects spoofing, and performs OCR.",
      "endpoint": "POST /v1/read_document",
      "source": "Manappuram API Spec",
      "curl": "curl -X POST {{URL}}/v1/read_document \\\n-H 'Authorization: Bearer <Token>' \\\n-F image=@image.jpg \\\n-F app_id=app_id \\\n-F doc_type=1 \\\n-F journey=na \\\n-F referenceId=123456 \\\n-F is_back=0 \\\n-F app_type=A",
      "inputs": [
        {"Param": "image", "M/O": "M", "Description": "Image of Aadhaar card"},
        {"Param": "doc_type", "M/O": "M", "Description": "1 - Aadhaar"},
        {"Param": "is_back", "M/O": "M", "Description": "0 - Front, 1 - Back"},
        {"Param": "journey", "M/O": "M", "Description": "na - Non-aadhaar, ea - E-aadhaar"}
      ],
      "outputs": {
        "result": [
          {
            "details": {
              "aadhaar": {"value": "xxxx", "conf": 0},
              "name": {"value": "Name", "conf": 0},
              "dob": {"value": "DD-MM-YYYY", "conf": 0}
            },
            "type": "aadhaar_front_bottom"
          }
        ],
        "status": "success",
        "statusCode": "200"
      }
    },
