[
    {
      "title": "PAN NSDL Verification",
      "description": "Verify PAN details via NSDL.",
      "endpoint": "POST /v1/verify_pan_number",
      "source": "Verification APIs Doc",
      "curl": "curl --location '{url}/v1/verify_pan_number' \\\n--header 'Authorization: {Bearer Auth token}' \\\n--form 'app_id=\"\"' \\\n--form 'referenceId=\"\"' \\\n--form 'name=\"\"' \\\n--form 'father_name=\"\"' \\\n--form 'pan_number=\"\"' \\\n--form 'dob=\"\"'",
      "inputs": [
        {"Param": "pan_number", "M/O": "M", "Description": "10 char PAN"},
        {"Param": "name", "M/O": "M", "Description": "Full Name"},
        {"Param": "father_name", "M/O": "M", "Description": "Father's Name"},
        {"Param": "dob", "M/O": "M", "Description": "Date of Birth"}
      ],
      "outputs": {
        "result": {
          "outputData": [
            {
              "pan": "ABCDE1234F",
              "pan_status": "E",
              "name": "Y",
              "fathername": "",
              "dob": "Y",
              "seeding_status": "Y"
            }
          ]
        },
        "status": "success",
        "statusCode": 200
      }
    },
    {
      "title": "Passport Verification API",
      "description": "Verify passport details via file number and DOB.",
      "endpoint": "POST /v1/passport_verification",
      "source": "Verification APIs Doc",
      "curl": "curl --location '{url}/v1/passport_verification' \\\n--header 'Authorization: Bearer {auth token}' \\\n--form 'app_id=\"app_id\"' \\\n--form 'referenceId=\"123456\"' \\\n--form 'file_no=\"DLXXXXXXXXXXXXX\"' \\\n--form 'dob=\"01/01/2000\"'",
      "inputs": [
        {"Param": "file_no", "M/O": "M", "Description": "Passport File Number"},
        {"Param": "dob", "M/O": "M", "Description": "Date of Birth"}
      ],
      "outputs": {
        "data": {
          "application_recieved_date": "01/01/2023",
          "file_number": "DLXXXXXXXXXXXXX",
          "passport_number": "RXXXXXXX",
          "status": "Passport RXXXXXXX has been dispatched..."
        },
        "status": "success",
        "statusCode": 200
      }
    },
    {
