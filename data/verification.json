      "inputs": [
        {"Param": "dl_number", "M/O": "M", "Description": "DL Number"},
        {"Param": "dob", "M/O": "M", "Description": "Date of Birth"},
        {"Param": "state", "M/O": "M", "Description": "State Name"}
